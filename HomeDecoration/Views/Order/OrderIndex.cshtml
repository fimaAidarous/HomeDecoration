@model IEnumerable<HomeDecoration.Models.DTO.Order>

@{
    ViewBag.Title = "Order";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Order </h2>
<table class="table">
    <tr>
        <th>#</th>
        <th>Product Name</th>
        <th>Customer Name</th>
        <th>Order_Date</th>
        <th>Total_Amoun</th>
    </tr>
    @foreach (var order in Model)
    {
        <tr>
            <td>@order.Id</td>
            <td>@order.Product.Name</td>
            <td>@order.Customer.Name</td>
            <td>@order.Order_Date</td>
            <td>@order.Total_Amount</td>
        </tr>
    }
</table>
